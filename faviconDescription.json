{
  "masterPicture": "images/leaf.svg",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#ffffff",
      "margin": "14%",
      "assets": {
        "ios6AndPriorIcons": true,
        "ios7AndLaterIcons": true,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": true
      },
      "appName": "AOLCCBC"
    },
    "desktopBrowser": {
      "design": "raw"
    },
    "windows": {
      "pictureAspect": "whiteSilhouette",
      "backgroundColor": "#ff0000",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": true,
        "windows10Ie11EdgeTiles": {
          "small": true,
          "medium": true,
          "big": true,
          "rectangle": true
        }
      },
      "appName": "AOLCCBC"
    },
    "androidChrome": {
      "pictureAspect": "shadow",
      "themeColor": "#ffffff",
      "manifest": {
        "name": "AOLCCBC",
        "display": "browser",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": false,
        "lowResolutionIcons": true
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "silhouette",
      "themeColor": "#48a548"
    }
  },
  "settings": {
    "compression": 5,
    "scalingAlgorithm": "Spline",
    "errorOnImageTooSmall": false,
    "readmeFile": true,
    "htmlCodeFile": true,
    "usePathAsIs": false
  }
}
